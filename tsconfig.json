{
  "compilerOptions": {
    "alwaysStrict": true,
    "declaration": true,
    "declarationDir": "dist/typings",
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "module": "ESNext",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "strictNullChecks": true
  },
  "lib": [
    "es2015"
  ]
}